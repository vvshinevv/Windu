<resources>

    <!-- Base application theme. -->



   <style parent="Theme.AppCompat.Light.NoActionBar" name="AppTheme">

    <!-- Customize your theme here. -->


    </style>

    <style name="starRatingBar" parent="@android:style/Widget.RatingBar">
        <item name="android:indeterminateOnly">false</item>
        <item name="android:progressDrawable">@drawable/custom_ratingbar_full</item>
        <item name="android:minHeight">22dp</item>
        <item name="android:maxHeight">22dp</item>

        <item name="android:adjustViewBounds">true</item>
        <item name="android:thumb">@null</item>
    </style>

    <style name="Theme.Transparent" parent="android:Theme">
        <item name="android:windowBackground">@android:color/transparent</item>
        <item name="android:colorBackgroundCacheHint">@null</item>
        <item name="android:windowIsTranslucent">true</item>
        <item name="android:windowAnimationStyle">@android:style/Animation</item>
        <item name="android:windowNoTitle">true</item>
        <item name="android:windowContentOverlay">@null</item>
        <item name="android:backgroundDimEnabled">false</item>
    </style>


</resources>
